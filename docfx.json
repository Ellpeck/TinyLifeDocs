{
    "metadata": [{
        "src": [{
            "src": "../",
            "files": ["**/TinyLife.csproj"]
        }],
        "dest": "api"
    }],
    "build": {
        "content": [{
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md",
                    "api/**.yml"
                ]
            },
            {
                "files": ["**.md"],
                "src": "Changelog"
            }
        ],
        "resource": [{
            "files": [
                "favicon.ico",
                "banner.png",
                "logo.png"
            ]
        }],
        "globalMetadata": {
            "_appTitle": "Tiny Life Docs",
            "_appLogoPath": "logo.png",
            "_appFooter": "<a href=\"https://github.com/Ellpeck/TinyLifeDocs\">&copy; 2021 Ellpeck</a> &ndash; <a href=\"https://ellpeck.de/#impressum\">Impressum</a> &ndash; <a href=\"https://ellpeck.de/#privacy\">Privacy</a>"
        },
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "template"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false
    }
}